# Fig Environment Configuration
# Copy this file to .env and fill in the values for your deployment.
# IMPORTANT: Never commit the .env file to source control.

# Database name for Fig
FIG_DB_NAME=fig

# Database login username
FIG_USER_NAME=fig_login

# Database password â€” generate a strong, unique password
FIG_DB_PWD=<GENERATE-A-STRONG-PASSWORD>

# SQL Server SA password (used for initial container setup)
SA_PWD=<SET-SA-PASSWORD>

# Fully qualified domain name for the deployment
fqdn=<SET-MACHINE-NAME>
